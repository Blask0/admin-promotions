{
  "name": "admin-promotions",
  "vendor": "vtex",
  "version": "0.4.9",
  "title": "Promotions Admin",
  "description": "Admin interface for Promotions",
  "mustUpdateAt": "2019-08-27",
  "registries": ["smartcheckout"],
  "scripts": {},
  "builders": {
    "messages": "1.x",
    "react": "3.x",
    "admin": "0.x"
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.admin-promotions-graphql": "0.x"
  },
  "policies": [
    {
      "name": "read_prices"
    },
    {
      "name": "modify_prices"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/{{account}}/conditions/{{type}}/condition/{{conditionId}}"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog_system/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/rnb/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "logistics.vtexcommercestable.com.br",
        "path": "/api/logistics/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexpayments.com.br",
        "path": "/api/pvt/merchants/payment-systems"
      }
    }
  ],
  "billingOptions": {
    "free": true
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
